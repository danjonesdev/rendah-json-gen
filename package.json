{
  "name": "front",
  "version": "1.0.0",
  "description": "Frontend build system for internal use.",
  "repository": {
    "type": "git",
    "url": "https://github.com/MosquitoDigitalLtd/front.git"
  },
  "license": "UNLICENSED",
  "paths": {
    "src": {
      "root": "./src/",
      "js": "./src/static/scripts/",
      "css": "./src/static/styles/",
      "images": "./src/static/images/",
      "fonts": "./src/static/fonts/",
      "templates": "./src/templates/"
    },
    "dist": {
      "root": "./build/",
      "js": "./build/static/scripts/",
      "css": "./build/static/styles/",
      "images": "./build/static/images/",
      "fonts": "./build/static/fonts/"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "flickity": "^2.1.1",
    "flickity-imagesloaded": "^2.0.0",
    "js-cookie": "^2.2.0",
    "superagent": "^3.8.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-webpack-plugin": "^0.1.1",
    "browser-sync": "^2.24.4",
    "chai": "^4.1.2",
    "cross-env": "^5.1.6",
    "css-mqpacker": "^6.0.2",
    "cssnano": "^3.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-eslint": "^4.0.2",
    "gulp-htmlmin": "^4.0.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^7.0.1",
    "gulp-prettify": "^0.5.0",
    "gulp-sass": "^4.0.1",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-stylelint": "^7.0.0",
    "gulp-twig": "^1.2.0",
    "gulp-util": "^3.0.8",
    "jsdom": "^11.11.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "postcss-reporter": "^5.0.0",
    "postcss-scss": "^1.0.5",
    "run-sequence": "^2.2.1",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.10.2",
    "webpack-stream": "^4.0.3"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production gulp build --production",
    "build-docs": "cross-env NODE_ENV=production gulp build --production --docs",
    "watch": "gulp watch",
    "watch-docs": "gulp watch --docs",
    "lint": "gulp eslint && gulp stylelint",
    "test": "mocha --require babel-core/register 'test/**/*.test.js' || true"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "IE 10"
    ]
  }
}
